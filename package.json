{
  "name": "@martindoyleuk/web-toolkit",
  "version": "0.0.2",
  "description": "Configuration files and utilities for web projects, especially React based",
  "main": "dist/index.js",
  "files": [
    "commitlint.config.js",
    ".prettierrc.js"
  ],
  "scripts": {
    "checktypes": "tsc --noEmit --project tsconfig.src.json",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:watch": "nodemon --config bin/nodemon/eslint.json",
    "eslint": "eslint .",
    "hook:pre-commit": "npm run lint",
    "hook:pre-push": "npm run verify",
    "lint": "run-p --aggregate-output --print-label prettier:check eslint checktypes",
    "prepare": "husky install",
    "prettier:base": "prettier --config .prettierrc.js --no-editorconfig --ignore-path .gitignore --ignore-unknown \"**/*.*\"",
    "prettier:check": "npm run prettier:base -- --check",
    "prettier:fix": "npm run prettier:base -- --write",
    "release": "npm publish --acces=public",
    "test": "echo \"Error: no tests specified\"",
    "verify": "npm test",
    "version": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MartinDoyleUK/web-toolkit.git"
  },
  "keywords": [
    "config"
  ],
  "author": "Martin Doyle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MartinDoyleUK/web-toolkit/issues"
  },
  "homepage": "https://github.com/MartinDoyleUK/web-toolkit#readme",
  "dependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@sindresorhus/tsconfig": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "commander": "^7.2.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.3",
    "eslint-plugin-jest-async": "^1.0.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-regexp": "^0.7.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
